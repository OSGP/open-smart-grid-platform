      <xsd:enumeration value="POWER_FAILURE"/>
      <!-- 1 - power down g -  -  - SMART_METER_G -->
      <xsd:enumeration value="POWER_FAILURE_G"/>
      <!-- 1 - power down w -  -  - SMART_METER_W -->
