      <!-- 0x8380 - M-Bus device on channel 4, has status bit 0 set (battery low) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_0_BATTERY_LOW_CHANNEL_4"/>
      <!-- 0x8381 - M-Bus device on channel 4, has status bit 1 set (battery consumption too high) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_1_BATTERY_CONSUMPTION_TOO_HIGH_CHANNEL_4"/>
      <!-- 0x8382 - M-Bus device on channel 4, has status bit 2 set (reverse flow) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_2_REVERSE_FLOW_CHANNEL_4"/>
      <!-- 0x8383 - M-Bus device on channel 4, has status bit 3 set (tamper P2) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_3_TAMPER_P2_CHANNEL_4"/>
      <!-- 0x8384 - M-Bus device on channel 4, has status bit 4 set (tamper P0) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_4_TAMPER_P0_CHANNEL_4"/>
      <!-- 0x8385 - M-Bus device on channel 4, has status bit 5 set (tamper case) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_5_TAMPER_CASE_CHANNEL_4"/>
      <!-- 0x8386 - M-Bus device on channel 4, has status bit 6 set (tamper magnetic) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_6_TAMPER_MAGNETIC_CHANNEL_4"/>
      <!-- 0x8387 - M-Bus device on channel 4, has status bit 7 set (temp out of range) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_7_TEMP_OUT_OF_RANGE_CHANNEL_4"/>
      <!-- 0x8388 - M-Bus device on channel 4, has status bit 8 set (clock sync error) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_8_CLOCK_SYNC_ERROR_CHANNEL_4"/>
      <!-- 0x8389 - M-Bus device on channel 4, has status bit 9 set (sw error) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_9_SW_ERROR_CHANNEL_4"/>
      <!-- 0x838A - M-Bus device on channel 4, has status bit 10 set (watchdog error) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_10_WATCHDOG_ERROR_CHANNEL_4"/>
      <!-- 0x838B - M-Bus device on channel 4, has status bit 11 set (system / hw error) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_11_SYSTEM_HW_ERROR_CHANNEL_4"/>
      <!-- 0x838C - M-Bus device on channel 4, has status bit 12 set (CFG calibration error) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_12_CFG_CALIBRATION_ERROR_CHANNEL_4"/>
      <!-- 0x838D - M-Bus device on channel 4, has status bit 13 set (high flow > Qmax) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_13_HIGH_FLOW_GREATER_THAN_QMAX_CHANNEL_4"/>
      <!-- 0x838E - M-Bus device on channel 4, has status bit 14 set (temp sensor error) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_14_TEMP_SENSOR_ERROR_CHANNEL_4"/>
      <!-- 0x838F - M-Bus device on channel 4, has status bit 15 set (reserved) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_15_RESERVED_CHANNEL_4"/>
      <!-- 0x8390 - M-Bus device on channel 4, has status bit 16 set (P0 enabled) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_16_P0_ENABLED_CHANNEL_4"/>
      <!-- 0x8391 - M-Bus device on channel 4, has status bit 17 set (new key accepted) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_17_NEW_KEY_ACCEPTED_CHANNEL_4"/>
      <!-- 0x8392 - M-Bus device on channel 4, has status bit 18 set (new key rejected) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_18_NEW_KEY_REJECTED_CHANNEL_4"/>
      <!-- 0x8393 - M-Bus device on channel 4, has status bit 19 set (reserved) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_18_RESERVED_CHANNEL_4"/>
      <!-- 0x8394 - M-Bus device on channel 4, has status bit 20 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_20_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x8395 - M-Bus device on channel 4, has status bit 21 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_21_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x8396 - M-Bus device on channel 4, has status bit 22 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_22_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x8397 - M-Bus device on channel 4, has status bit 23 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_23_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x8398 - M-Bus device on channel 4, has status bit 24 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_24_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x8399 - M-Bus device on channel 4, has status bit 25 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_25_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x839A - M-Bus device on channel 4, has status bit 26 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_26_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x839B - M-Bus device on channel 4, has status bit 27 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_27_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x839C - M-Bus device on channel 4, has status bit 28 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_28_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x839D - M-Bus device on channel 4, has status bit 29 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_29_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x839E - M-Bus device on channel 4, has status bit 30 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_30_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x839F - M-Bus device on channel 4, has status bit 31 set (manufacturer specific event) -->
      <xsd:enumeration value="MBUS_STATUS_BIT_31_MANUFACTURER_SPECIFIC_CHANNEL_4"/>
      <!-- 0x83A0 - Key is sent to the M-Bus device on channel 4 -->
      <xsd:enumeration value="KEY_SENT_TO_MBUS_DEVICE_ON_CHANNEL_4"/>
      <!-- 0x83A1 - Key is acknowledged by the M-Bus device on channel 4 -->
      <xsd:enumeration value="KEY_ACKNOWLEDGED_BY_MBUS_DEVICE_ON_CHANNEL_4"/>

      <!-- the following events are System Events -->
      <xsd:enumeration value="SET_MAINTENANCE_STATE_E_SUCCESS"/>
      <xsd:enumeration value="SET_MAINTENANCE_STATE_G_SUCCESS"/>
      <xsd:enumeration value="SET_MAINTENANCE_STATE_E_FAILED"/>
      <xsd:enumeration value="SET_MAINTENANCE_STATE_G_FAILED"/>
      <xsd:enumeration value="COMMUNICATION_NETWORK_INFORMATION_UPDATED"/>
      <xsd:enumeration value="CLEAN_UP_M_BUS_CHANNEL"/>
      <xsd:enumeration value="FIRMWARE_UPDATE_STARTED"/>
      <xsd:enumeration value="INVOCATION_COUNTER_THRESHOLD_REACHED"/>

    </xsd:restriction>
  </xsd:simpleType>

  <!-- Data Types -->

  <xsd:complexType name="Device">
    <xsd:sequence>
      <xsd:element name="DeviceUid" type="xsd:string"/>
      <xsd:element name="DeviceIdentification" type="common:Identification"/>
      <xsd:element name="DeviceType" type="xsd:string"/>
      <xsd:element name="Activated" type="xsd:boolean"/>
      <xsd:element name="HasSchedule" type="xsd:boolean"/>
      <xsd:element name="PublicKeyPresent" type="xsd:boolean"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="DevicePage">
    <xsd:sequence>
      <xsd:element name="TotalPages" type="xsd:int"/>
      <xsd:element maxOccurs="unbounded" name="Devices"
                   type="tns:Device"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="MessageLog">
    <xsd:sequence>
      <xsd:element name="Timestamp" type="xsd:dateTime"/>
      <xsd:element name="OrganisationIdentification" type="common:Identification"/>
      <xsd:element name="DeviceIdentification" type="common:Identification"/>
      <xsd:element name="DecodedMessage" type="xsd:string"/>
      <xsd:element name="EncodedMessage" type="xsd:string"/>
      <xsd:element name="Incoming" type="xsd:boolean"/>
      <xsd:element name="Valid" type="xsd:boolean"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="MessageLogPage">
    <xsd:sequence>
      <xsd:element name="TotalPages" type="xsd:int"/>
      <xsd:element maxOccurs="unbounded" name="MessageLogs"
                   type="tns:MessageLog"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SetDeviceCommunicationSettingsData">
    <xsd:sequence>
      <xsd:element type="xsd:integer" name="ChallengeLength"/>
      <xsd:element type="xsd:boolean" name="WithListSupported"
                   default="false"/>
      <xsd:element type="xsd:boolean" name="SelectiveAccessSupported"
                   default="false"/>
      <xsd:element type="xsd:boolean" name="IpAddressIsStatic"
                   default="false"/>
      <xsd:element type="xsd:boolean" name="UseSn" default="false"/>
      <xsd:element type="xsd:boolean" name="UseHdlc"
                   default="false"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="SetDeviceLifecycleStatusByChannelRequestData">
    <xsd:complexContent>
      <xsd:extension base="common:Query">
        <xsd:sequence>
          <xsd:element name="DeviceLifecycleStatus" type="common:DeviceLifecycleStatus"/>
          <xsd:element name="Channel" type="common:Channel"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="SetDeviceLifecycleStatusByChannelResponseData">
    <xsd:complexContent>
      <xsd:extension base="common:Response">
        <xsd:sequence>
          <xsd:element name="GatewayDeviceIdentification"
            type="common:Identification"/>
          <xsd:element name="MbusDeviceIdentification" type="common:Identification"/>
          <xsd:element name="DeviceLifecycleStatus" type="common:DeviceLifecycleStatus"/>
          <xsd:element name="Channel" type="common:Channel"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="ClearMbusStatusRequestData">
    <xsd:complexContent>
      <xsd:extension base="common:Query">
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="ClearMbusStatusResponseData">
    <xsd:complexContent>
      <xsd:extension base="common:Response">
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="GetGsmDiagnosticRequestData">
    <xsd:complexContent>
      <xsd:extension base="common:Query"/>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:element name="GetGsmDiagnosticRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:DeviceIdentification"/>
        <xsd:element name="GetGsmDiagnosticRequestData"
          type="tns:GetGsmDiagnosticRequestData"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="GetGsmDiagnosticAsyncResponse">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="common:AsyncResponse"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="GetGsmDiagnosticAsyncRequest">
    <xsd:complexType>
      <xsd:complexContent>
        <xsd:extension base="common:AsyncRequest"/>
      </xsd:complexContent>
    </xsd:complexType>
  </xsd:element>

  <xsd:element name="GetGsmDiagnosticResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="Result" type="common:OsgpResultType"/>
        <xsd:element name="GetGsmDiagnosticResponseData"
          type="tns:GetGsmDiagnosticResponseData"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <xsd:complexType name="GetGsmDiagnosticResponseData">
    <xsd:complexContent>
      <xsd:extension base="common:Response">
        <xsd:sequence>
          <xsd:element name="Operator" type="xsd:string"/>
          <xsd:element name="ModemRegistrationStatus" type="tns:ModemRegistrationStatus"/>
          <xsd:element name="CircuitSwitchedStatus" type="tns:CircuitSwitchedStatus"/>
          <xsd:element name="PacketSwitchedStatus" type="tns:PacketSwitchedStatus"/>
          <xsd:element name="CellInfo" type="tns:CellInfo"/>
          <xsd:element name="AdjacentCells" type="tns:AdjacentCellInfo" maxOccurs="unbounded"/>
          <xsd:element name="CaptureTime" type="xsd:dateTime"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="CellInfo">
    <xsd:sequence>
      <xsd:element name="CellId" type="xsd:unsignedInt"/>
      <xsd:element name="LocationId" type="xsd:unsignedShort"/>
      <xsd:element name="SignalQuality" type="tns:SignalQuality"/>
      <xsd:element name="BitErrorRate" type="tns:BitErrorRate"/>
      <xsd:element name="MobileCountryCode" type="xsd:unsignedShort"/>
      <xsd:element name="MobileNetworkCode" type="xsd:unsignedShort"/>
      <xsd:element name="ChannelNumber" type="xsd:unsignedInt"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="AdjacentCellInfo">
    <xsd:sequence>
      <xsd:element name="CellId" type="xsd:unsignedInt"/>
      <xsd:element name="SignalQuality" type="tns:SignalQuality"/>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:simpleType name="ModemRegistrationStatus">
    <xsd:restriction base="xsd:string">
      <!-- 0 - Not registered -->
      <xsd:enumeration value="NOT_REGISTERED"/>
      <!-- 1 - Registered home network -->
      <xsd:enumeration value="REGISTERED_HOME_NETWORK"/>
      <!-- 2 - Not registered, but MT is currently search new operator to register to -->
      <xsd:enumeration value="NOT_REGISTERED_CURRENT_SEARCH_OPERATOR_TO_REGISTER_TO"/>
      <!-- 3 - Registration denied -->
      <xsd:enumeration value="REGISTRATION_DENIED"/>
      <!-- 4 - Unknown -->
      <xsd:enumeration value="UNKNOWN"/>
      <!-- 5 - Registered roaming -->
      <xsd:enumeration value="REGISTERED_ROAMING"/>
      <!-- 6-255 - Reserved -->
      <xsd:enumeration value="RESERVED"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="CircuitSwitchedStatus">
    <xsd:restriction base="xsd:string">
      <!-- 0 - Inactive -->
      <xsd:enumeration value="INACTIVE"/>
      <!-- 1 - Incoming call -->
      <xsd:enumeration value="INCOMING_CALL"/>
      <!-- 2 - Active -->
      <xsd:enumeration value="ACTIVE"/>
      <!-- 3-255 - Reserved -->
      <xsd:enumeration value="RESERVED"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="PacketSwitchedStatus">
    <xsd:restriction base="xsd:string">
      <!-- 0 - Inactive -->
      <xsd:enumeration value="INACTIVE"/>
      <!-- 1 - GPRS -->
      <xsd:enumeration value="GPRS"/>
      <!-- 2 - EDGE -->
      <xsd:enumeration value="EDGE"/>
      <!-- 3 - UMTS -->
      <xsd:enumeration value="UMTS"/>
      <!-- 4 - HSDPA -->
      <xsd:enumeration value="HSDPA"/>
      <!-- 5 - LTE -->
      <xsd:enumeration value="LTE"/>
      <!-- 6 - CDMA -->
      <xsd:enumeration value="CDMA"/>
      <!-- 7 - LTE Cat M1 -->
      <xsd:enumeration value="LTE_CAT_M_18"/>
      <!-- 8 - LTE NB-IoT -->
      <xsd:enumeration value="LTE_NB_IOT"/>
      <!-- 9-255 - Reserved -->
      <xsd:enumeration value="RESERVED"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="SignalQuality">
    <xsd:restriction base="xsd:string">
      <!-- 0 - -113 dBm or less -->
      <xsd:enumeration value="MINUS_113_DBM_OR_LESS"/>
      <!-- 1 - -111 dBm -->
      <xsd:enumeration value="MINUS_111_DBM"/>
      <!-- 2 - -109 dBm -->
      <xsd:enumeration value="MINUS_109_DBM"/>
      <!-- 3 - -107 dBm -->
      <xsd:enumeration value="MINUS_107_DBM"/>
      <!-- 4 - -105 dBm -->
      <xsd:enumeration value="MINUS_105_DBM"/>
      <!-- 5 - -103 dBm -->
      <xsd:enumeration value="MINUS_103_DBM"/>
      <!-- 6 - -101 dBm -->
      <xsd:enumeration value="MINUS_101_DBM"/>
      <!-- 7 - -99 dBm -->
      <xsd:enumeration value="MINUS_99_DBM"/>
      <!-- 8 - -97 dBm -->
      <xsd:enumeration value="MINUS_97_DBM"/>
      <!-- 9 - -95 dBm -->
      <xsd:enumeration value="MINUS_95_DBM"/>
      <!-- 10 - -93 dBm -->
      <xsd:enumeration value="MINUS_93_DBM"/>
      <!-- 11 - -91 dBm -->
      <xsd:enumeration value="MINUS_91_DBM"/>
      <!-- 12 - -89 dBm -->
      <xsd:enumeration value="MINUS_89_DBM"/>
      <!-- 13 - -87 dBm -->
      <xsd:enumeration value="MINUS_87_DBM"/>
      <!-- 14 - -85 dBm -->
      <xsd:enumeration value="MINUS_85_DBM"/>
      <!-- 15 - -83 dBm -->
      <xsd:enumeration value="MINUS_83_DBM"/>
      <!-- 16 - -81 dBm -->
      <xsd:enumeration value="MINUS_81_DBM"/>
      <!-- 17 - -79 dBm -->
      <xsd:enumeration value="MINUS_79_DBM"/>
      <!-- 18 - -77 dBm -->
      <xsd:enumeration value="MINUS_77_DBM"/>
      <!-- 19 - -75 dBm -->
      <xsd:enumeration value="MINUS_75_DBM"/>
      <!-- 20 - -73 dBm -->
      <xsd:enumeration value="MINUS_73_DBM"/>
      <!-- 21 - -71 dBm -->
      <xsd:enumeration value="MINUS_71_DBM"/>
      <!-- 22 - -69 dBm -->
      <xsd:enumeration value="MINUS_69_DBM"/>
      <!-- 23 - -67 dBm -->
      <xsd:enumeration value="MINUS_67_DBM"/>
      <!-- 24 - -65 dBm -->
      <xsd:enumeration value="MINUS_65_DBM"/>
      <!-- 25 - -63 dBm -->
      <xsd:enumeration value="MINUS_63_DBM"/>
      <!-- 26 - -61 dBm -->
      <xsd:enumeration value="MINUS_61_DBM"/>
      <!-- 27 - -59 dBm -->
      <xsd:enumeration value="MINUS_59_DBM"/>
      <!-- 28 - -57 dBm -->
      <xsd:enumeration value="MINUS_57_DBM"/>
      <!-- 29 - -55 dBm -->
      <xsd:enumeration value="MINUS_55_DBM"/>
      <!-- 30 - -53 dBm -->
      <xsd:enumeration value="MINUS_53_DBM"/>
      <!-- 31 - -51 dBm or greater -->
      <xsd:enumeration value="MINUS_51_DBM_OR_GREATER"/>
      <!-- 99 - not known or not detectable -->
      <xsd:enumeration value="NOT_KNOWN_OR_NOT_DETECTABLE"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:simpleType name="BitErrorRate">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="RXQUAL_0"/>
      <xsd:enumeration value="RXQUAL_1"/>
      <xsd:enumeration value="RXQUAL_2"/>
      <xsd:enumeration value="RXQUAL_3"/>
      <xsd:enumeration value="RXQUAL_4"/>
      <xsd:enumeration value="RXQUAL_5"/>
      <xsd:enumeration value="RXQUAL_6"/>
      <xsd:enumeration value="RXQUAL_7"/>
      <!-- 99 - not known or not detectable -->
      <xsd:enumeration value="NOT_KNOWN_OR_NOT_DETECTABLE"/>
    </xsd:restriction>
  </xsd:simpleType>

</xsd:schema>
