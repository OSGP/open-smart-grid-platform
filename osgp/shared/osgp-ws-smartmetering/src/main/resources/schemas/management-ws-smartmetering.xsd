  </xsd:element>

  <xsd:element name="SetDeviceCommunicationSettingsRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="DeviceIdentification" type="common:DeviceIdentification"/>
        <xsd:element name="SetDeviceCommunicationSettingsData"
