<jxb:bindings
  version="2.0"
  xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
  xmlns:xs="http://www.w3.org/2001/XMLSchema">

  <jxb:bindings schemaLocation="../schemas/sm-configuration.xsd">
    <jxb:schemaBindings>
      <jxb:package name="org.opensmartgridplatform.adapter.ws.schema.smartmetering.configuration"></jxb:package>
    </jxb:schemaBindings>

    <jxb:bindings
      node="//xs:complexType[@name='UpdateFirmwareRequestData']//xs:element[@name='FirmwareVersion']">
      <jxb:property name="firmwareVersions" />
    </jxb:bindings>

    <jxb:bindings node="//xs:element[@name='UpdateFirmwareRequest']//xs:element[@name='FirmwareVersion']">
      <jxb:property name="firmwareVersions" />
    </jxb:bindings>

  </jxb:bindings>

</jxb:bindings>
