<soap:Envelope
  xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/"
  xmlns:ns="http://www.opensmartgridplatform.org/schemas/security/secretmanagement">
  <soap:Header>
    <ns:correlationUid>123456</ns:correlationUid>
  </soap:Header>
  <soap:Body>
    <ns:storeSecretsRequest>
      <!-- Note that this request contains secrets that are swapped compared
        to the data generated by the predefined createTestData() method in SoapServiceSecretManagementIT.
        This is done to avoid errors triggered by trying to store the same secret
        twice successively for a device and secret type. -->
      <ns:DeviceId>E0000000000000000</ns:DeviceId>
      <ns:TypedSecrets>
        <ns:TypedSecret>
          <ns:Type>E_METER_AUTHENTICATION_KEY</ns:Type>
          <ns:Secret></ns:Secret>
        </ns:TypedSecret>
        <ns:TypedSecret>
          <ns:Type>E_METER_ENCRYPTION_KEY_UNICAST</ns:Type>
          <ns:Secret></ns:Secret>
        </ns:TypedSecret>
      </ns:TypedSecrets>
    </ns:storeSecretsRequest>
  </soap:Body>
</soap:Envelope>
