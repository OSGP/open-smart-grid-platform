            <wsdl:input name="GetPQValuesPeriodicAsyncRequest">
                <soap:header message="tns:GenericHeader" part="OrganisationIdentification" use="literal"/>
                <soap:header message="tns:GenericHeader" part="UserName" use="literal"/>
